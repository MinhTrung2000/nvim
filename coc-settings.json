{
  "diagnostic.errorSign": ">>",
  "diagnostic.warningSign": "--",
  "diagnostic.enable": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.enableSign": true,
  "diagnostic.messageTarget": "float",
  "dialog.pickerButtons": true,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.format": "[%source%code] [%severity] %message",
  "diagnostic.displayByAle": false,
  "diagnostic.virtualText": false,
  "diagnostic.showDeprecated": true,
  "diagnostic.showUnused": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "graphql",
    "json",
    "python"
  ],
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnTypeFiletypes": [
    "css",
    "markdown",
    "javascript",
    "graphql",
    "json",
    "python"
  ],
  "coc.preferences.hoverTarget": "float",
  "pyright.enable": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.diagnosticMode": "openFilesOnly",
  "python.analysis.stubPath": "typings",
  "python.analysis.typeshedPaths": [],
  "python.analysis.diagnosticSeverityOverrides": {},
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.useLibraryCodeForTypes": true,
  "python.analysis.extraPaths": ["./sources", "./src", "./source"],
  "python.pythonPath": "python",
  "python.venvPath": "$HOME/anaconda3/bin/python",
  "python.sortImports.path": "$HOME/anaconda3/bin/isort",
  "python.formatting.provider": "yapf",
  "python.formatting.yapfPath": "/home/tmtcoder/anaconda3/bin/yapf",
  "python.formatting.blackPath": "/home/tmtcoder/anaconda3/bin/black",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": false,
  "python.linting.banditEnabled": false,
  "python.linting.mypyEnabled": false,
  "python.linting.pytypeEnabled": false,
  "python.linting.prospectorEnabled": false,
  "python.linting.pydocstyleEnabled": false,
  "python.linting.pylamaEnabled": false,
  "python.linting.pylintEnabled": false,
  "python.linting.lintOnSave": true,
  "pyright.organizeimports.provider": "pyright",
  "snippets.ultisnips.directories": ["UltiSnips", "~/.config/nvim/snips"],
  "snippets.userSnippetsDirectory": "~/.config/nvim/snips",
  "javascript.showUnused": true,
  "javascript.suggest.paths": true,
  "javascript.format.enabled": true,
  "javascript.validate.enable": true,
  "javascript.suggest.autoImports": true,
  "javascript.suggestionActions.enabled": true,
  "javascript.preferences.importModuleSpecifier": "auto"
}
